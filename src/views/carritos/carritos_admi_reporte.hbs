{{#if Libros}}
<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <ul class="list-group">
            {{# each Libros}}
            <li class="list-group-item">
                <strong> Reporte 1 </strong>
                <p> Tema: {{this.item.temaLibro}} </p>
                <p> Cantidad: {{this.cantidad}}</p>
                <p>Precio: {{this.precioLibro}}</p>
            </li>
            {{/each}}
        </ul>
    </div>
</div>

<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <ul class="list-group">
            <!{{# each Libros}}>
                <li class="list-group-item">
                    <strong> Reporte 2 </strong>
                    <p>  Cliente: {{this.user.nombreUser}} </p>
                    <p> Rango: [1:3] </p>
                    <p>Cliente con más pedidos realizados: {{this.user.nombreUser}} </p>
                </li>
                <!{{/each}}>
        </ul>
    </div>
</div>

<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <ul class="list-group">
            {{# each Libros}}
            <li class="list-group-item">
                <strong> Reporte 3 </strong>
                <p>Libro: {{this.item.nombreLibro}} </p>
                <p>Cantidad de veces pedido: {{this.cantidad}} </p>
            </li>
            {{/each}}
        </ul>
    </div>
</div>

<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <ul class="list-group">
            <li class="list-group-item">
                <strong> Reporte 4 </strong>
                <p>Total pedidos realizados: {{totalCantidadLibro}} </p>
                <p>Cliente con más pedidos realizados: {{this.user.nombreUser}} </p>
            </li>
        </ul>
    </div>
</div>


{{else}}
<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <h2> No hay reportes </h2>
    </div>
</div>
{{/if}}
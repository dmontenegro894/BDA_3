<div class="row">
    {{#each promociones}}
    <div class='col-md-3'>
        <div class='card'>
            <div class='card-body'>
                <h4 class='card-tittle'>
                    {{codigoPromocion}} 
                </h4>
                <p> {{descripcionPromocion}}
                </p>
                <p> {{fechaInicioPromocion}}
                </p>
                <p> {{fechaFinPromocion}}
                </p>
                <p> {{descuentoPromocion}}
                </p>
            </div>
        </div>
    </div>
    {{else}}
     <div class = 'card mx-auto'>
         <div class = 'card-body'>
             <p class="lead"> No hay promociones</p>
         </div>
     </div>
    {{/each}}
</div>